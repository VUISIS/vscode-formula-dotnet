{
  "name": "formula",
  "displayName": "Formula",
  "description": "Formula Notebook",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "keywords": [
    "notebookRenderer"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:formula-notebook"
  ],
  "main": "./out/extension/extension.js",
  "browser": "./out/extension/extension.web.js",
  "contributes": {
    "languages": [
      {
        "id": "formula-lang",
        "aliases": [
          "Formula"
        ],
        "filenamePatterns": [
          "*.4ml"
        ]
      }
    ],
    "notebooks": [
      {
        "type": "formula-notebook",
        "displayName": "Formula Notebook",
        "selector": [
          {
            "filenamePattern": "*.4mlnb"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "webpack --mode development --watch",
    "pretest": "webpack --mode development && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@nteract/messaging": "^7.0.20",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.21",
    "@types/vscode": "^1.64.0",
    "@types/vscode-notebook-renderer": "^1.57.8",
    "@types/webpack-env": "^1.16.3",
    "@types/zeromq": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "css-loader": "^4.2.0",
    "eslint": "^8.6.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "monaco-editor": "^0.32.1",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "rxjs": "^7.5.4",
    "split2": "^4.1.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "util": "^0.12.4",
    "vscode-notebook-error-overlay": "^1.0.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "zeromq": "^6.0.0-beta.6"
  }
}
