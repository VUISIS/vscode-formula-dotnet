{
  "name": "formula-dotnet",
  "displayName": "formula-dotnet",
  "description": "Formula Language Extension",
  "publisher": "vanderbilt",
  "repository": "https://github.com/VUISIS/vscode-formula-dotnet",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:formula"
  ],
  "main": "./out/extension/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "formula",
        "aliases": [
          "Formula",
          "formula"
        ],
        "extensions": [
          ".4ml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "formula",
        "scopeName": "source.formula",
        "path": "./syntaxes/formula.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "formula",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm run compile:extension",
    "compile:extension": "tsc -b",
    "lint": "eslint src --ext ts",
    "watch": "concurrently -r \"npm:watch:*\"",
    "watch:extension": "tsc -b --watch",
    "watch:client": "webpack --mode development --watch",
    "dev": "concurrently -r npm:watch:extension",
    "pretest": "npm run compile && npm run lint"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.57.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "concurrently": "^6.3.0",
    "css-loader": "^4.2.0",
    "eslint": "^7.27.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "vscode-uri": "^3.0.2"
  }
}
