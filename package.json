{
  "name": "formula",
  "displayName": "Formula",
  "description": "Formula Notebook",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "keywords": [
    "notebook"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:formula",
    "onNotebook:formula-notebook"
  ],
  "main": "./out/extension/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "formula",
        "extensions": [
          ".4ml"
        ]
      }
    ],
    "notebooks": [
      {
        "type": "formula-notebook",
        "displayName": "Formula Notebook",
        "selector": [
          {
            "filenamePattern": "*.4mlnb"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "kernel.restart",
        "title": "Restart Kernel"
      },
      {
        "command": "save.solution",
        "title": "Save Solution"
      }
    ],
    "menus": {
      "notebook/cell/title": [
        {
          "command": "save.solution",
          "group": "navigation"
        }
      ],
      "notebook/toolbar": [
        {
          "command": "kernel.restart",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "kernel.restart",
          "when": "false"
        },
        {
          "command": "save.solution",
          "when": "false"
        }
      ]
    }
  },
  "semanticHighlighting": true,
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode development",
    "lint": "eslint src --ext ts",
    "dev": "",
    "pretest": "webpack --mode development && npm run lint"
  },
  "devDependencies": {
    "@nteract/messaging": "^7.0.20",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.21",
    "@types/vscode": "^1.64.0",
    "@types/webpack-env": "^1.16.3",
    "@types/zeromq": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "concurrently": "^7.0.0",
    "eslint": "^8.6.0",
    "file-loader": "^6.2.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "rxjs": "^7.5.4",
    "split2": "^4.1.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "util": "^0.12.4",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0",
    "zeromq": "^6.0.0-beta.6"
  },
  "dependencies": {
    "web-tree-sitter": "0.20.5"
  }
}
